{
    "rewrites": [
    {
      "source": "/post/:slug",
      "destination": "/api/post/[slug]"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/post/:slug",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ],
  
  
    "redirects": [
      {
        "source": "/",
        "destination": "https://example.com",
        "permanent": false,
        "has": [
          {
            "type": "host",
            "value": "voice-info-blog.vercel.app"
          }
        ]
      }
    ]
  }