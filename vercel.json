{
    "rewrites": [
      {
        "source": "/(.*)",
        "destination": "/index.html"
      }
    ],
  
  
    "redirects": [
      {
        "source": "/",
        "destination": "https://example.com",
        "permanent": false,
        "has": [
          {
            "type": "host",
            "value": "voice-info-blog.vercel.app"
          }
        ]
      }
    ]
  }