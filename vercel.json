{
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/(.*)",
      "destination": "https://www.voiceinfos.com/$1",
      "permanent": false,
      "has": [
        {
          "type": "host",
          "value": "voice-info-blog.vercel.app"
        }
      ]
    },
    {
      "source": "/(.*)",
      "destination": "https://www.voiceinfos.com/$1",
      "permanent": false,
      "has": [
        {
          "type": "host",
          "value": "voice-info-blog-fwpt8jvbr-daudu-qazims-projects.vercel.app"
        }
      ]
    }
  ]
}